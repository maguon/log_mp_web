}

.check{
  height: 40px;
  width: 94%;
  display: flex;
  background: white;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
  margin: 0px 3% 0px 3%;
}
.default-redact{
  font-size: 12px;
  text-align: center;
  width: 15%;
  height: 20px;
  color: #874086;
  border: 1px solid #874086; 
  opacity: 0.9;     /*透明度变化*/
  transform: scale(0.9,0.9); /*缩放*/
}




 
