page{  
  display: flex;  
  flex-direction: column;  
  height: 100%;  
 background: #efeff4;
}  

.label{
width: 100%;
height: 110px;
background: white;
margin-top: 3px;
}
.scroll{
height: 85%;
}


.product{
width: 100%;
height: 110px;
display: flex;
flex-direction: column;
}
.default{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 15px;
  margin-top: 5px;
}
.default-item{
  color: #feb100;
  margin-left: 5px;
  font-size: 15px;
}
.check{
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  margin-right: 15px;
}
.default-item-two{
  margin-left: 5px;
  font-size: 15px;
  color: #8a8988;
}
.default-redact{
  font-size: 12px;
  text-align: center;
  width: 12%;

  color: white;
  background:#c8a0c8; 
  opacity: 0.9;     /*透明度变化*/
  transform: scale(0.9,0.9); /*缩放*/
}

.ui-list-item-address{
  width: 90%;
  height: 40px;

  margin: 10px 25px;
  color: #8a8a8a;
  font-size: 15px;
}

.add-address{
  margin: 0 auto;
  margin-top: 10px;
  margin-right: 10px;
  width: 140px;
  height: 30px;
  background: white;
  line-height: 30px;
  text-align: center;
  color: #c8a0c8;
  border-radius: 10rpx;
  display: block;
  font-size: 15px;

}
.add-img{
  margin-right: 15rpx;
  width: 15px;
  height: 15px;
  position: relative;
  top: 2px;
}
 .weui-cell_active{
  background-color:#ececec;
  }
  
  .btn{
    position: fixed;
    bottom: 20px;
    left: 5%;
    height: 40px;
    width: 90%;
    background: #874086;
    color: white;
}
  .weui-cell_active_btn{
  background-color: #e6e6e6;
  }

  