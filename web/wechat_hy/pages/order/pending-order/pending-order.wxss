page{
    background: #f8f6f8;
}
.dis-head{
  width: 100%; 
  height: 60px;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.dis-line{
  width: 100%;
  height: 1px;
  background: #e8e8e9;
}
.head-city{
  font-size: 18px;
  margin-left: 15px;
  color: #7a387a;
}
.head-staycar{
  margin-left: 15px;
  width: 45px;
  height: 15px;
  font-weight: bolder;
}
.head-customer{
  width: 28px;
  height: 20px;
  position: absolute;
  right: 20px;
}
/* ---------------------------------------------------------------------- */

.dis-way{
  width: 100%;
  height: 50px;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: white;
}
.way-image{
  width: 25px;
  height: 18px;
  margin-left: 15px;
  margin-right: 15px;
}
.way-text{
  font-size: 15px;
  color: #8a8a8a;
}
/* ---------------------------------------------------------------------- */
.dis-content{
 width: 100%;
 height: 50px;
 background: white;
}
.dis-content01{
 width: 100%;
 height: 80px;
 background: white;
 display: flex;
 flex-direction: column;


}
.city-arrow{
  width:18px;
  height:18px;
}
.arrow{
  width: 20px;
  height: 20px;
  position: absolute;
  right: 2px;
}
.active{
background-color:#ececec;
}
.city{
 width: 100%;
 height: 40px;

 display: flex;
 flex-direction: row;
 align-items: center;
}
.city-img{
  width: 20px;
  height: 20px;
  margin-left: 15px;
}
.city-text{
  font-size: 15px;
  color: #8a8a8a;
  margin-left: 15px;
  margin-right: 10px;
}

.stay-expect{
  display: flex;
  flex-direction: row;
  align-content: center;
  position: absolute;
  right: 30px;
}

.stay-car{
  width: 100%;
  height: 40px;
  line-height: 40px;
  font-size:15px;
  color: #8a8a8a; 
  margin-left: 15px;
}

.stay-text{
  font-size:15px;
  color: #8a8a8a; 
}
.stay-num{
  color: #d43286;
  font-size: 20px;
  margin-top: -5px;
}
/* ---------------------------------------------------------------------- */
.dis-sum{
width: 100%;
height: 50px;
background: white;
display: flex;
flex-direction: row;
align-items: center;
}
.sum-text{
font-size: 15px;
color: #8a8a8a;
margin-left: 15px;
}
.sum-text01{
  color: #8a8a8a;
}
.sum-line{
  width: 100%;
  height: 2px;
  background: #c5c4c5;
}
/* ---------------------------------------------------------------------- */
.note{
  width: 100%;
  height: 50px;
  background: white;
  display: flex;
  align-items: center;
}
.note-text{
  width: 10%;
  font-size: 15px;
  margin-left: 15px;
  color: #8a8a8a;
}
.note-input{
  font-size: 15px;
   color: #c0bcbc;
   width: 80%;
}
/* ---------------------------------------------------------------------- */

.dis-add{
  width: 100%;
  height: 60px;
  background: white;
}
.add-button{
  width: 120px;
  height: 30px;
  position: absolute;
  right: 10px;
  margin-top: 15px;
  border: 1px solid #7a387a;
}
.add-text{
  font-size: 15px;
  color: #7a387a;
  position: relative;
  bottom: 5px;
}
/* ---------------------------------------------------------------------- */
.dis-orderid{
  width: 100%;
  height: 80px;
}
.orderid{
  width: 100%;
  height: 40px;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.orderid-text{
  font-size: 14px;
  color: #8a8a8a;
  margin-left: 15px;
}
.orderid-button{
  width: 55px;
  height: 20px;
  position: absolute;
  right: 10px;
}
.button-text{
  font-size: 12px;
  color: #8a8a8a;
  position: relative;
  top:-5px;
}
.create-time{
  font-size: 12px;
  color: #b4b3b3;
  margin-left: 15px;
}
/* ---------------------------------------------------------------------- */
.dis-cancel{
  width: 100%;
  height: 45px;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0px;
  background:#f7f5f7; 
}
.cancel{
  width: 50%;
  font-size: 18px;
  color: #d43286;
  z-index:1000000;
}
.submit{
  width: 50%;
  font-size: 18px;
  color:white;
  background: #7a387a;
}
button[disabled]{
  background-color: #aaa5a5 !important; 
}
.cancel_btn{
  background-color: #aaa5a5;
}