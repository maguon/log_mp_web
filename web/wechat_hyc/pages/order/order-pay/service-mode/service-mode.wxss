
.dis-head{
  width: 100%; 
  height: 60px;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.dis-line{
  width: 100%;
  height: 1px;
  background: #e8e8e9;
}

.head-city{
  font-size: 18px;
  margin-left: 15px;
  color: #7a387a;
}
.head-staycar{
  margin-left: 15px;
  width: 20px;
  height: 10px;
  font-weight: bolder;
}
.head-customer{
  width: 28px;
  height: 20px;
  position: absolute;
  right: 20px;
}
/* ---------------------------------------------------------------------- */

.dis-way{
  width: 100%;
  height: 50px;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: white;
}
.way-image{
  width: 25px;
  height: 18px;
  margin-left: 15px;
  margin-right: 15px;
}
.way-text{
  font-size: 15px;
  color: #8a8a8a;
}
/* ---------------------------------------------------------------------- */

.ress{
  width: 100%;
  height: 100px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.ress01{
  width: 100%;
  height: 80px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.ress-people{
display: flex;
flex-direction: row;
}
.ress-img{
height: 20px;
width: 20px;
margin-left: 10px;
}
.ress-img01{
height: 20px;
width: 30px;
margin-left: 10px;
}
.people{
font-size: 15px;
margin-left: 10px;
}
.people01{
font-size: 15px;
margin-left: 2px;
}

.arrow{
height: 20px;
width: 20px;
position: absolute;
right: 3px;
margin-top: 40px;
}
.arrow01{
height: 20px;
width: 20px;
position: absolute;
right: 3px;
}
.phone{
margin-left: 40px;
margin-top: 10px;
font-size: 14px;
color: #8a8a8a;
}
.address{
margin: 10px 30px 0px 40px;
font-size: 14px;
color: #8a8a8a;
}
.ress-line{
  width: 100%;
  height: 3px;
  background: #f8f6f8;
}
.ress-msg{
  width: 100px;
  height: 30px;
  font-size: 12px;
  color: #8a8a8a;
  margin-top: 10px;
  position: absolute;
  right: 20px;
}
/* <!-- ---------------------------------------------------------------------- --> */
.butlist{
  position: fixed;
  bottom: 0px;
  height: 70px;
  width: 100%;
  background: #f7f5f7;

}
.btn-area{
  position: fixed;
  left: 1%;
  right: 1%;
  bottom: 20px;
  height: 45px;
  width: 90%;
  background: #7a387a;
  color: white;
}

.weui-cell_active{
  background-color:#ececec;
  }
  .null{
    height: 120px;
    width: 100%;
  }
/* <!-- ---------------------------------------------------------------------- --> */
.choose{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.choose-img{
  height: 20px;
  width: 20px;
  margin-left: 30px;
}
.choose-ress{
  margin-left: 30px;
  font-size: 15px;
  color: #d43286;
}

/* <!-- ---------------------------------------------------------------------- --> */
.note{
  width: 100%;
  height: 50px;
  background: white;
  display: flex;
  align-items: center;
}

.refund{
  width: 80px;
  height: 25px;
  font-size: 12px;
  line-height: 25px;
  text-align: center;
  position: absolute;
  right: 20px;
  color: #8a8a8a;
  border:  1px solid #ccc9c9;
}