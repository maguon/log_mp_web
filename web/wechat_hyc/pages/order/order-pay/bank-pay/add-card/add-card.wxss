page{
  background: #efeff4;
}
/* ---------------------------------------------------------------------- */
.bank{
  height: 100px;
  margin-left: 2%;
  margin-right: 2%;
  margin-top: 10px;
  margin-bottom: 10px;
  border-radius: 2%;
  background: #864c86;
}
.prompt{
  width: 100%;
  text-align: center;
  font-size: 15px;
  color: #8a8a8a;
  margin-top: 50px;
}
.bank01{
  height: 100px;
  margin-left: 2%;
  margin-right: 2%;
  margin-top: 10px;
  margin-bottom: 10px;
  border-radius: 2%;
  background: white;
  border: 1px solid #864c86;
}
.bank-msg{
  height: 50px;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 15px;
}
.bank-img{
  height: 25px;
  width: 25px;
  margin-left: 10px;
}
.bank-name{
  font-size: 15px;
  color: white;
  margin-left: 10px;
}
.bank-left{
  width: 80%;
  height: 50px;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.bank-state{
  width: 30px;
  border:1px solid  #864c86;
  border-radius: 5px;
  position: absolute;
  right: 50px;
  color: #864c86;
  font-size: 12px;
  text-align: center;
}
.name{
  font-size: 15px;
  color: #cca8cc;
  position: absolute;
  right: 30px;
}
.bank-num{
  width: 70%;
  line-height: 50px;
  font-size: 18px;
  color: #cca8cc;
  margin-left: 15px;
}
.bank-sure{
  height:10px;
  width: 10px;
  position: absolute;
  right: 20px;
}
.color{
  color: #864c86;
}
/* ---------------------------------------------------------------------- */
.add{
  height: 40px;
  width: 100%;
  margin-top: 2px;
  background: white;
  display: flex;
  flex-direction: row;
  align-items: center;
  
}
.add-img{
height: 15px;
width: 15px;
color: #8a8a8a;
margin-left: 15px;
}
.add-text{
color: #df70b5;
margin-left: 20px;
font-size: 15px;
}
.arrow{
  height: 20px;
  width: 20px;
  position: absolute;
  right: 20px;
}

.active{
  background-color:#a158a1;
}
