.dis-head{
  width: 100%; 
  height: 60px;
  background: #f8f6f8;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.dis-line{
  width: 100%;
  height: 1px;
  background: #e8e8e9;
}
.head-city{
  font-size: 18px;
  margin-left: 15px;
  color: #7a387a;
}
.head-staycar{
  margin-left: 15px;
  width: 45px;
  height: 15px;
  font-weight: bolder;
}
.head-customer{
  width: 28px;
  height: 20px;
  position: absolute;
  right: 20px;
}
/* ---------------------------------------------------------------------- */

.dis-way{
  width: 100%;
  height: 50px;
  background: #f8f6f8;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.way-image{
  width: 25px;
  height: 18px;
  margin-left: 15px;
  margin-right: 15px;
}
.way-text{
  font-size: 15px;
  color: #8a8a8a;
}
/* ---------------------------------------------------------------------- */
.dis-content{
 width: 100%;
 height: 80px;
}
.arrow{
  width: 20px;
  height: 20px;
  position: absolute;
  right: 2px;
  margin-top: 15px;
}
.active{
background-color:#ececec;
}
.city{
 width: 100%;
 height: 50px;
 display: flex;
 flex-direction: row;
 align-items: center;
}
.city-img{
  width: 20px;
  height: 20px;
  margin-left: 15px;
}
.city-text{
  font-size: 15px;
  color: #8a8a8a;
  margin-left: 10px;
  margin-right: 10px;
}
.city-arrow{
  width:18px;
  height:18px;
}

.stay-detail{
  width: 100%;
  height: 30px;
  display: flex;
  flex-direction: row;
  align-content: center;
}
.stay-expect{
  display: flex;
  flex-direction: row;
  align-content: center;
  position: absolute;
  right: 30px;
}
.stay-car{
  width: 120px;
  font-size:12px;
  color: #8a8a8a; 
  margin-left: 50px;
}
.stay-text{
  font-size:12px;
  color: #8a8a8a; 
}
.stay-num{
  color: #d43286;
  font-size: 20px;
  margin-top: -7px;
}
/* ---------------------------------------------------------------------- */
.dis-sum{
width: 100%;
height: 50px;
display: flex;
flex-direction: row;
align-items: center;
}
.sum-text{
font-size: 15px;
color: #cc7da2;
margin-left: 15px;
}
.sum-line{
   width: 100%;
  height: 2px;
  background: #dfdfe2;

}
/* ---------------------------------------------------------------------- */
.dis-prompt{
  width: 100%;
  height: 60px;
  background:  #f8f6f8;
  display: flex;
  justify-content: center;
  align-items: center;
}
.prompt-text{
  width: 95%;
  font-size: 12px;
  color: #a5a1a1;
}
/* ---------------------------------------------------------------------- */

.dis-add{
  width: 100%;
  height: 60px;
}
.add-button{
  width: 120px;
  height: 30px;
  position: absolute;
  right: 10px;
  margin-top: 15px;
  border: 1px solid #7a387a;
}
.add-text{
  font-size: 15px;
  color: #7a387a;
  position: relative;
  bottom: 5px;
}
/* ---------------------------------------------------------------------- */
.dis-orderid{
  width: 100%;
  height: 60px;
  background: #f8f6f8;
}
.orderid{
  width: 100%;
  height: 40px;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.orderid-text{
  font-size: 14px;
  color: #8a8a8a;
  margin-left: 15px;
}
.orderid-button{
  width: 55px;
  height: 20px;
  position: absolute;
  right: 10px;
}
.button-text{
  font-size: 12px;
  color: #8a8a8a;
  position: relative;
  top:-5px;
}
.create-time{
  font-size: 12px;
  color: #b4b3b3;
  margin-left: 15px;
}
/* ---------------------------------------------------------------------- */
.dis-cancel{
  width: 100%;
  height: 45px;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0px;
  background:#f7f5f7; 
  border-top: 1px solid #dfdfe2;
}
.cancel{
  font-size: 18px;
  color: #d43286;
}
.cancel_btn{
  background-color: #aaa5a5;
}