page{
background: #efeff4;
}
.head{
  width: 100%;
  height: 80px;
  position: fixed;
  top: 0px;
  z-index: 1000;
  display: flex;
  flex-direction: row;
  align-items: center;
  background:linear-gradient(#7a387a,#7a387a);
}
.null{
  width: 100%;
  height: 80px;
}
.items{
  width: 20%;
  height:80px;
}
.prompt{
  width: 100%;
  height: 40px;
  text-align: center;
  font-size: 15px;
  color: #d1689e;
  line-height: 40px;
  font-weight:bolder;
}
.item{
  width: 100%;
  height:80px;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.item-img{
  width: 30px;
  height: 28px;
  margin-left: 2px;
  margin-top: 10px;
}
.item-text{
  font-size: 12px;
  margin-top: 10px;
  color: white;
}
.item-point{
  position: relative;
  top: -28px;
  right: -22px;
  width: 7px;
  height: 7px;
  background: red;
  border-radius: 50%;
}
.divline{
  width: 100%;
  height: 4px;
  margin-top: 11px;
  background:white;
}
/* <!--  ---------------------------------------------------------------------- --> */

.content{
 width: 100%;
 height: 120px;
 background: white;
 margin-bottom: 5px;
 /* border-top: 1px solid #ececec; */
}
.state{
  position: absolute;
  right: 20px;
  font-size: 12px;
  color: #d43286;
}
.state01{
  position: absolute;
  right: 20px;
  font-size: 12px;
  color: #8a8a8a;
}
.arrow{
  width: 20px;
  height: 20px;
  position: absolute;
  right: 1px;
  margin-top: 5px;
}
.active{
background-color:#ececec;
}

.city{
 width: 100%;
 height: 40px;
 display: flex;
 flex-direction: row;
 align-items: center;
}

.city-img{
  width: 25px;
  height: 25px;
  margin-left: 15px;
}
.city-text{
  font-size: 18px;
  margin-left: 10px;
  margin-right: 10px;
}
.city-arrow{
  width: 15px;
  height: 10px;
}

.stay-detail{
  width: 100%;
  height: 40px;
  display: flex;
  flex-direction: row;
  align-content: center;
}
.stay-expect{
  display: flex;
  flex-direction: row;
  align-content: center;
  position: absolute;
  right: 20px;
}
.stay-car{
  width: 120px;
  font-size:13px;
  color: #8a8a8a; 
  margin-left: 50px;
  margin-top: 10px;
}
.stay-text{
  font-size:13px;
  color: #8a8a8a; 
  margin-top: 10px;

}
.shadow{
  width: 100%;
  height: 4px;
  background:linear-gradient(#8d798d,#fafafa);
  position: fixed;
  top:80px;
}
.stay-num{
  color: #d43286;
  font-size: 13px;
  margin-top: 5px;
  text-decoration: blink;
}
.stay-num01{
  color: #8a8a8a;
  font-size: 15px;
  margin-top: 8px;

}

.content-text{
  width: 100%;
  height: 100px;
  font-size:15px;
  text-align: center;
  margin-top: 50px;
  color: #8a8a8a; 
}
.num{
  font-size: 12px;
  color: #8a8a8a;
  margin-left: 10px;
}
.num-time{
  position: absolute;
  right: 10px;
  color:#8a8a8a;
  font-size: 12px;
}