.content{
  width: 100%;
 height: 50px;
 background: #f8f6f8;
 display: flex;
 flex-direction: row;
 align-items: center;
}
.item{
  margin-left: 15px;
  margin-right: 15px;
  font-size: 18px;
  font-weight: bolder;
  color: #7a387a;
}
.item-img{
width: 25px;
height: 15px;
}
.city{
 width: 100%;
 height: 40px;

 display: flex;
 flex-direction: row;
 align-items: center;
}
.city-text{
  font-size: 15px;
  margin-left: 15px;
  margin-right: 10px;
}
.city-text01{
  font-size: 14px;
  margin-left: 15px;
  color: #8a8a8a;
}

.active{
background-color:#ececec;
}
.stay-text01{
  font-size:15px;
  color: #8a8a8a;
  position: absolute;
  right: 20px;
}
.item-text{
  font-size:14px;
  color: #8a8a8a;
  position: absolute;
  right: 20px;
}
.arrow{
  width: 20px;
  height: 20px;
  position: absolute;
  right: 2px;
}
.dis-line{
  width: 100%;
  height: 2px;
  background: #e8e8e9;
}
.dis-line01{
  width: 100%;
  height: 5px;
  background: #e8e8e9;
}
.city-input{
  font-size: 15px;
  color: #8a8a8a;
  text-align: right;
  position: absolute;
  right: 20px;
}
.remark{
  width: 90%;
  height: 60px;
  font-size: 12px;
  margin: 10px 15px 10px 15px;
}
.prompt{
  font-size: 14px;
  /* margin: 20px 5% 0px 5%; */
