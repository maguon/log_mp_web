  margin-top: 20px;
  color: #d43286;
}

.butlist{
  position: fixed;
  bottom: 0px;
  height: 70px;
  width: 100%;

}
.list{
  position: fixed;
  bottom: 0px;
  height: 60px;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
