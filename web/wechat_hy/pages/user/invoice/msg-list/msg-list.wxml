
<view class='add'>
<view class='add-text'>新增发票抬头信息</view>
<image bindtap='addAddress'  class='arrow' src='../../../../../images/add_02.png'></image>
</view>

<view  wx:for="{{invList}}" wx:key='u'>
<view bindtap="radioChange" data-index="{{index}}" class="label" hover-class='weui-cell_active_btn'>    
    

         <view class='default'>
         <image hidden='{{item.status}}' class='arc' src='.././../../../images/arc.png'></image>
           <image hidden='{{!item.status}}' class='arc01' src='.././../../../images/arc_01.png'></image>
         <view class='city'>{{item.company_name}}</view>
             <view hidden='{{!item.status}}' class='default-item'>默认</view>
             <view hidden='{{item.status}}' class='default-item-two'>设为默认</view>
         </view>
         <view class='div-line'></view>

<!-- <view class='item-ress'>
<view class='ress'>发票抬头：</view>
<view class='ress-text'>{{item.company_name}}</view>
</view> -->

<view class='item-ress'>
<view class='ress'>税<text decode="{{true}}" space="{{true}}">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</text>号：</view>
<view class='ress-text'>{{item.tax_number}}</view>
</view>

<view class='item-ress'>
<view class='ress'>开户银行：</view>
<view class='ress-text'>{{item.bank}}</view>
</view>

<view class='item-ress'>
<view class='ress'>银行账户：</view>
<view class='ress-text'>{{item.bank_code}}</view>
</view>


<view class='item-ress01'>
<view class='ress'>企业地址：</view>
<view class='ress-text'>{{item.company_address}}</view>
</view>

<view class='item-ress'>
<view class='ress'>电话号码：</view>
<view class='ress-text'>{{item.company_phone}}</view>
</view>
  
  </view>

   <view class='div-line'></view>

           <view class='check'>
            <view bindtap="editor" class='default-redact'data-id="{{index}}"  >编辑</view>
             <view bindtap="del" class="default-redact" data-id="{{index}}" >删除</view>
          </view>
  </view>
<view class='null'></view>

<view class="btnGroup">
 <button bindtap='useRess' class="btn" hover-class="weui-cell_active_btn">确定</button>
</view>



