page{
  background: #f8f6f8;
}
.state{
 width: 100%;
 height: 40px;
}
.state-text{
  font-size: 12px;
  color:#d175a0;
  line-height: 40px;
  position: absolute;
  right: 15px;
}

/* <!--  ---------------------------------------------------------------------- --> */
.content{
 width: 96%;
 height: 160px;
 margin-left: 1%;
 margin-right: 2%;
 background: white;
 border: 3px solid #f0f0f0;
}



.city{
 width: 100%;
 height: 30px;
 display: flex;
 flex-direction: row;
 align-items: center;
}

.city-img{
  width: 25px;
  height: 25px;
  margin-left: 15px;
}
.city-text{
  font-size: 15px;
  margin-left: 10px;
  margin-right: 10px;
}
.city-arrow{
  width: 15px;
  height: 10px;
}

.stay-detail{
  width: 100%;
  height: 30px;
  display: flex;
  flex-direction: row;
  align-content: center;
}
.stay-expect{
  display: flex;
  flex-direction: row;
  align-content: center;
  position: absolute;
  right: 25px;
}
.stay-car{
  width: 120px;
  font-size:13px;
  color: #8a8a8a; 
  margin-left: 10px;
  margin-top: 10px;
}
.stay-text{
  font-size:13px;
  color: #8a8a8a; 
  margin-top: 10px;

}
.stay-num{
  color: #d43286;
  font-size: 18px;
  margin-top: 5px;
  text-decoration: blink;
}
.stay-num01{
  color: #d43286;
  font-size: 18px;
  margin-top: 5px;
    text-decoration:line-through;
}

.content-text{
  width: 100%;
  height: 100px;
  font-size:15px;
  text-align: center;
  margin-top: 50px;
  color: #8a8a8a; 
}
.num{
  font-size: 12px;
  color: #8a8a8a;
  margin-left: 10px;
}
.num-time{
  position: absolute;
  right: 15px;
  color:#8a8a8a;
  font-size: 12px;
}
.dis-line{
  border: 1px solid #f1f1f1;
  position: absolute;
  right: 4%;
  left: 4%;
}
.line{
  border: 1px solid #f1f1f1;
}
/* <!--  ---------------------------------------------------------------------- --> */
.item{
 width: 100%;
 height: 40px;
 display: flex;
 flex-direction: row;
 align-items: center;
 background: white;
 margin-top: 10px;
}
.active{
background-color:#ececec;
}
.invoice-head{
  font-size: 15px;
  color:#d175a0;
  line-height: 40px;
  margin-left: 15px;
}
.arrow{
  width: 20px;
  height: 20px;
  position: absolute;
  right: 1px;
}
.item-ress{
  display: flex;
  flex-direction: row;
}
.item-tiny{
 width: 100%;
 height: 165px;
 display: flex;
 font-size: 12px;
 color: #8a8a8a;
 flex-direction: column;
 background: white;
}
.ress{
width:70px;
margin-left: 15px;
 margin-top: 8px;
}
.ress-text{
 margin-top: 8px;
 
}

/* <!--  ---------------------------------------------------------------------- --> */
.note{
  width: 100%;
  height: 40px;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: white;
}
.note01{
  height:25px;
}
.note-right{
  font-size: 12px;
  margin-left: 15px;
}
.note-left{
  width: 80%;
  color: #8a8a8a;
  font-size: 12px;
  margin-left: 15px;
}
/* <!--  ---------------------------------------------------------------------- --> */
.butList{
  margin-top: 20px;
  height: 60px;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;

}

.undo{
width: 40%;
height: 40px;
border: 1px solid #d43286;
color: #d43286;
font-size: 18px;
display: flex;
align-items: center;
justify-content: center;
}
.again{
width: 40%;
height: 40px;
color: white;
font-size: 18px;
background: #7a387a;
display: flex;
align-items: center;
justify-content: center;
}