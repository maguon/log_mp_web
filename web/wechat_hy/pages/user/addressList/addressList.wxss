page{  
 background: #efeff4;
}  

.label{
width: 94%;
height: 160px;
background: white;
margin: 10px 3% 0px 3%;
display: flex;
flex-direction: column;
}
.default{
  height: 40px;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.city{
  font-size: 15px;
  margin-left: 15px;
}
.default-item{
  width: 35px;
  text-align: center;
  color: #df70b5;
  margin-left: 2px;
  font-size: 12px;
  border: 1px solid #df70b5;
  border-radius: 5px;
  position: absolute;
  right: 20px;
}
.div-line{
  border: 1px solid #efeff4;
}
.default-item-two{
  width: 60px;
  text-align: center;
  color: #8a8a8a;
  margin-left: 2px;
  font-size: 12px;
  border: 1px solid #8a8a8a;
  border-radius: 5px;
  position: absolute;
  right: 20px;
}
.item-address{
  width: 90%;
  height: 40px;
  margin: 10px 15px;
  color: #8a8a8a;
  font-size: 15px;
}
.name{
  margin-left: 15px;
  font-size: 15px;
}
.phone{
position: absolute;
right: 20px;
color: #8a8a8a;
font-size: 15px;
}

.check{
  height: 40px;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
}

.default-redact{
  font-size: 12px;
  text-align: center;
  width: 15%;
  height: 20px;
  color: #874086;
  border: 1px solid #874086; 
  opacity: 0.9;     /*透明度变化*/
  transform: scale(0.9,0.9); /*缩放*/
}




 .weui-cell_active{
  background-color:#ececec;
  }
  .btnGroup{
    width: 100%;
    height: 60px;
    background: #efeff4;
    position: fixed;
    bottom:0px;
}
  .btn{
    position: fixed;
    bottom: 10px;
    left: 5%;
    height: 40px;
    width: 90%;
    background: #874086;
    color: white;
}
  .weui-cell_active_btn{
  background-color: #e6e6e6;
  }


  .add{
  height: 40px;
  width: 100%;
  margin-top: 2px;
  background: white;
  display: flex;
  flex-direction: row;
  align-items: center;
  
}
.add-text{
color: #df70b5;
margin-left: 20px;
font-size: 15px;
}
.arrow{
  height: 20px;
  width: 20px;
  position: absolute;
  right: 20px;
}
.null{
  width: 100%;
  height: 200px;
}

  